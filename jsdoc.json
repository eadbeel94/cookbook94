{
  "plugins": ["better-docs/component","jsdoc-http-plugin"],
  "source": {
    "include": ["./src","./backend"],
    "includePattern": "\\.(jsx|js|ts|tsx)$",
    "excludePattern": "(public|node_modules|public|other|build)"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "recurse": "true",
    "destination": "./build/static/doc",
    "template": "node_modules/better-docs",
    "readme": "./README.jsdoc.md"
  }
}